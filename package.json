{
  "name": "sticker-printer",
  "version": "1.0.0",
  "description": "A kid-friendly drawing app with Bluetooth printer support",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:nobuild": "npx serve -p 8000",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.config.js",
    "test:browser": "vitest run --config vitest.browser.config.js",
    "test:ui": "vitest --ui --config vitest.config.js",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "keywords": [
    "drawing",
    "printer",
    "bluetooth",
    "kids"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/web-bluetooth": "^0.0.21",
    "@vitest/browser": "^4.0.8",
    "@vitest/browser-playwright": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "jsdom": "^27.1.0",
    "playwright": "^1.56.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "lit": "^3.3.1",
    "tiny-invariant": "^1.3.3"
  }
}
